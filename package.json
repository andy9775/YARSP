{
  "name": "yarsp",
  "version": "0.0.1",
  "description": "Yet Another React Starter Pack: A starter pack for ReactJS containing code splitting, testing and an api endpoint.",
  "main": "index.js",
  "scripts": {
    "test": "npm run lint && npm run test:coverage",
    "test:client": "npm run lint --silent && npm run test:cover:client --silent",
    "lint": "./node_modules/.bin/eslint ./app && ./node_modules/.bin/eslint ./test",
    "test:cover:client": "NODE_ENV=test SILENCE_CONSOLE=true ./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha --report html --report text test/**/*_spec.*.js",
    "test:cover:server": "NODE_ENV=test ./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha test/**/*_spec.*.js",
    "test:coverage": "npm run test:cover:server && (npm run test:coveralls ; npm run test:codecov ; npm run test:codeclimate)",
    "test:coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls --verbose",
    "test:codecov": "codecov",
    "test:codeclimate": "codeclimate-test-reporter < ./coverage/lcov.info",
    "test:run": "NODE_ENV=test ./node_modules/.bin/_mocha",
    "test:run:silent": "SILENCE_CONSOLE=true NODE_ENV=test ./node_modules/.bin/_mocha --silent",
    "test:watch": "NODE_ENV=test ./node_modules/.bin/_mocha --watch --reporter min",
    "dev": "NODE_ENV=development node index.js",
    "make-dir:build": "[ ! -d build/ ] && mkdir build/ || true",
    "clean-dir:build": "[ -d build/ ] && rm -r build/** || npm run make-dir:build",
    "make-dir:dist": "[ ! -d public/dist/ ] && mkdir public/dist/ || true",
    "clean-dir:dist": "[ -d public/dist/ ] && rm -r public/dist/** || npm run make-dir:dist",
    "babel": "./node_modules/.bin/babel ./app --out-dir ./build",
    "pack": "./node_modules/.bin/webpack --config webpack.config.prod.js",
    "build": "npm run clean-dir:build && npm run babel && npm run clean-dir:dist && npm run pack",
    "prod": "npm run build && NODE_ENV=production node index.js",
    "start": "npm run prod"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andy9775/YARSP.git"
  },
  "keywords": [
    "ReactJS",
    "Express.js",
    "Node"
  ],
  "author": "Andy Chrzaszcz <andy9775@gmail.com> (andyc.io)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andy9775/YARSP/issues"
  },
  "homepage": "https://github.com/andy9775/YARSP#readme",
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "codeclimate-test-reporter": "^0.4.0",
    "coveralls": "^2.11.15",
    "enzyme": "^2.6.0",
    "esformatter": "^0.10.0",
    "esformatter-add-trailing-commas": "^1.1.0",
    "esformatter-braces": "^1.2.1",
    "esformatter-collapse-objects": "^0.5.1",
    "esformatter-jquery-chain": "^1.1.0",
    "esformatter-jsx": "^7.2.0",
    "esformatter-literal-notation": "^1.0.1",
    "esformatter-parseint": "^1.0.3",
    "esformatter-quote-props": "^2.0.0",
